syn match Braces display '[{}]'
hi Braces ctermfg=grey
syn match Coma display ','
hi Coma ctermfg=grey
syn match Semicolon display ';'
hi Semicolon ctermfg=grey
syn match Parens display '[()]'
hi Parens ctermfg=grey
syn match Brackets display '[\[\]]'
hi Brackets ctermfg=red

set tabstop=2
set shiftwidth=2
set expandtab
